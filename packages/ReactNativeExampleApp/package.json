{
  "name": "ReactNativeExampleApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint './src/**/*.{ts,tsx}' --max-warnings 0",
    "lint:fix": "eslint './src/**/*.{ts,tsx}' --max-warnings 0 --fix",
    "lint:perf": "TIMING=1 npm run eslint",
    "prettier": "prettier '{,packages/**/}*.{md,js,ts,tsx,json}' --check",
    "prettier:fix": "prettier '{,packages/**/}*.{md,js,ts,tsx,json}' --write",
    "tslint": "tsc --pretty --noEmit",
    "test": "watchman watch-del-all && jest",
    "postinstall": "yarn rnuc .env",
    "rename": "npx react-native-rename 'ReactNativeExampleApp' -b 'com.example.rnapp'"
  },
  "dependencies": {
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/elements": "^1.3.21",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "date-fns": "^2.30.0",
    "i18next": "^23.7.6",
    "lodash": "^4.17.21",
    "react": "18.2.0",
    "react-i18next": "^13.4.1",
    "react-native": "0.72.6",
    "react-native-gesture-handler": "^2.13.4",
    "react-native-mmkv": "^2.11.0",
    "react-native-reanimated": "^3.5.4",
    "react-native-safe-area-context": "^4.7.4",
    "react-native-screens": "^3.27.0",
    "react-native-svg": "^14.0.0",
    "react-native-ultimate-config": "^6.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "@babel/preset-env": "^7.23.3",
    "@babel/runtime": "^7.23.2",
    "@react-native-template/eslint-config": "*",
    "@react-native-template/prettier-config": "*",
    "@react-native/metro-config": "^0.73.2",
    "@swc/core": "^1.3.96",
    "@swc/jest": "^0.2.29",
    "@testing-library/react-native": "^12.3.3",
    "@tsconfig/react-native": "^3.0.2",
    "@types/jest": "^29.5.8",
    "@types/lodash": "^4.14.201",
    "@types/react": "^18.2.37",
    "@types/react-test-renderer": "^18.0.6",
    "babel-jest": "^29.7.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.53.0",
    "jest": "^29.7.0",
    "metro-react-native-babel-preset": "0.77.0",
    "prettier": "^3.1.0",
    "react-test-renderer": "18.2.0",
    "typescript": "5.2.2"
  },
  "workspaces": {
    "nohoist": [
      "**/react-native-ultimate-config"
    ]
  }
}
